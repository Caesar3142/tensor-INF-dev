FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    body
    {
        type            calculated;
        value           uniform 101325;
    }
    inlet1
    {
        type            calculated;
        value           uniform 101325;
    }
    inlet2
    {
        type            calculated;
        value           uniform 101325;
    }
    outlet1
    {
        type            calculated;
        value           uniform 101325;
    }
}


